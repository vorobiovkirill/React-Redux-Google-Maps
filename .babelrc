{
	"env": {
		"production": {
			"presets": [
				[
					"es2015",
					{
						"modules": false
					}
				],
				"react",
				"stage-0"
			],
			"plugins": [
				"lodash",
				"transform-decorators-legacy", [
					"transform-runtime",
					{
						"polyfill": false,
						"regenerator": true
					}
				]
			]
		},
		"development": {
			"presets": [
				[
					"es2015",
					{
						"modules": false
					}
				],
				"react",
				"stage-0"
			],
			"plugins": [
				"react-hot-loader/babel",
				"lodash",
				"transform-decorators-legacy", [
					"transform-runtime",
					{
						"polyfill": false,
						"regenerator": true
					}
				]
			]
		},
		"test": {
			"presets": [
				"es2015",
				"react",
				"stage-0"
			],
			"plugins": [
				"react-hot-loader/babel",
				"lodash"
			]
		}
	}
}
